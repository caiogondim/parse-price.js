{
  "name": "parse-price",
  "version": "1.0.0",
  "description": "returns a `Number` from a localized price string",
  "main": "src/index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "tap test/index.js --cov",
    "test:lint": "standard 'src/**/*.js' 'test/**/*.js'",
    "test:lint:fix": "standard --fix 'src/**/*.js' 'test/**/*.js'",
    "test:all": "npm run test:lint && npm run test",
    "build": "webpack",
    "build:min": "webpack && uglifyjs dist/parse-price.js -c -m -o dist/parse-price.min.js",
    "build:all": "npm run build && npm run build:min"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/caiogondim/parse-price.js.git"
  },
  "keywords": [
    "parse",
    "number",
    "localization",
    "price",
    "i10n",
    "i18n",
    "internationalization"
  ],
  "author": "Caio Gondim",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/caiogondim/parse-price.js/issues"
  },
  "homepage": "https://github.com/caiogondim/parse-price.js#readme",
  "devDependencies": {
    "covert": "^1.1.0",
    "standard": "^7.1.2",
    "tap": "^6.3.0",
    "uglify-js": "^2.7.0",
    "webpack": "^1.13.1"
  }
}
